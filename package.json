{
  "name": "@sil/markdown-elements",
  "description": "Automatic markdown for specific elements",
  "author": "Sil van Diepen",
  "version": "1.0.0",
  "main": "dist/markdown-elements.umd.js",
  "module": "dist/markdown-elements.esm.js",
  "unpkg": "dist/markdown-elements.min.js",
  "browser": {
    "./sfc": "src/markdown-elements.vue"
  },
  "scripts": {
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/markdown-elements.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/markdown-elements.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/markdown-elements.min.js"
  },
  "dependencies": {
    "@sil/markdown": "^1.0.1"
  },
  "devDependencies": {
    "minimist": "^1.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16"
  },
  "license": "ISC"
}
